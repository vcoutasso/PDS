   "source": [
    "import sounddevice as sd\n",
    "import numpy as np\n",
    "import pds\n",
    "\n",
    "fs = 8192\n",
    "x_clean = np.load('data/123test.npy')\n",
    "x = np.load('data/123test_noise.npy')\n",
    "\n",
    "f0 = 1e3 \n",
    "w0 = 2 * np.pi * f0 / fs\n",
    "r = 0.9369586958695869\n",
    "\n",
    "y = pds.notch(x, w0, r)\n",
    "\n",
    "sd.play(y, fs, blocking=False)\n",
    "\n",
    "pds.mse(x_clean, y)"
   ]
  }
